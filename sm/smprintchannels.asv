function smprintchannels(inst)

global smdata;

if nargin < 1
    inst = 1:length(smdata.inst);
end

fmt = '%2d  %-10s  %-10s\n';
fprintf(['Inst', fmt(4:end)], 'Device', 'Dev. Name');
fprintf([repmat('-', 1, 60), '\n']);
for i = inst;
    fprintf(fmt, i, smdata.channels(i).name, smdata.inst(i).device, smdata.inst(i).name);
end
